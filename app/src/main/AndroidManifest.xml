<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="in.ac.ternaengg.concessions">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/tec_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/tec_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".StudentApplicationsActivity"></activity>
        <activity android:name=".StaffHomeActivity" />
        <activity android:name=".StaffLoginActivity" />
        <activity android:name=".StudentHomeActivity" />
        <activity android:name=".StudentLoginActivity" />
        <activity android:name=".ApplyConcessionActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>